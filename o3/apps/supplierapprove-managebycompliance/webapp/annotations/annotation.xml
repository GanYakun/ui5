<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Common.v1" Alias="Common"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Namespace="com.sap.vocabularies.UI.v1" Alias="UI"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Communication.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Communication.v1" Alias="Communication"/>
    </edmx:Reference>
    <edmx:Reference Uri="/officeauto/control/odataAppSvc/supplierApproveService/$metadata">
        <edmx:Include Namespace="com.dpbird"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="local">
        <Annotations Target="com.dpbird.SupplierParty">
            <Annotation Term="UI.SelectionFields">
                <Collection>
                    <PropertyPath>partyId</PropertyPath>
                    <PropertyPath>PartyClassification/PartyClassificationGroup/description</PropertyPath>
                </Collection>
            </Annotation>
            <Annotation Term="UI.LineItem" Qualifier="ComplianceAppLineItem">
                <Collection>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value">
                            <Path>partyId</Path>
                        </PropertyValue>
                        <Annotation Term="UI.Importance">
                            <EnumMember>UI.ImportanceType/High</EnumMember>
                        </Annotation>
                    </Record>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value">
                            <Path>PartyClassification/PartyClassificationGroup/description</Path>
                        </PropertyValue>
                        <PropertyValue Property="Criticality">
                            <Path>riskCritical</Path>
                        </PropertyValue>
                        <Annotation Term="UI.Importance">
                            <EnumMember>UI.ImportanceType/High</EnumMember>
                        </Annotation>
                    </Record>
                    <Record Type="UI.DataFieldWithUrl">
                        <PropertyValue Property="Label" String="DD Form"/>
                        <PropertyValue Property="Value" Path="ddFromName"/>
                        <PropertyValue Property="Url" Path="ddFromUrl"/>
                    </Record>
                </Collection>
            </Annotation>


             <Annotation Term="UI.HeaderFacets">
                <Collection>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="ViewDDForm"/>
                        <PropertyValue Property="Label" String="{@i18n>ViewDDForm}"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#ViewDDForm"/>
                    </Record>
                     <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="Classification"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.DataPoint#PartyClasification"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="Compliance Response"/>
                        <PropertyValue Property="ID" String="ComplianceResult"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#ComplianceResult"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="Work Scope"/>
                        <PropertyValue Property="ID" String="WorkScope"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#WorkScope"/>
                    </Record>
                </Collection>
            </Annotation>

            <Annotation Term="UI.DataPoint" Qualifier="PartyClasification">
                <Record Type="UI.DataPointType">
                    <PropertyValue Property="Value" Path="PartyClassification/PartyClassificationGroup/description"/>
                    <PropertyValue Property="Title" String="{@i18n>RiskRating}"/>
                    <PropertyValue Property="Criticality" Path="riskCritical"/>
                </Record>
            </Annotation>

            <Annotation Term="UI.DataPoint" Qualifier="cycleTime">
                <Record Type="UI.DataPointType">
                    <PropertyValue Property="Value" Path="cycleTime"/>
                    <PropertyValue Property="Title" String="{@i18n>cycleTime}"/>
                </Record>
            </Annotation>

            <Annotation Term="UI.Facets">
                <Collection>
                    <Record Type="UI.CollectionFacet">
                        <PropertyValue Property="ID" String="SupplierFacet"/>
                        <PropertyValue Property="Label" String="DD Form"/>
                        <PropertyValue Property="Facets" >
                            <Collection >
                                <Record Type="UI.ReferenceFacet">
                                    <PropertyValue Property="ID" String="DDFormBoolFields"/>
                                    <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#DDFormBoolFields"/>
                                </Record>
                                <Record Type="UI.ReferenceFacet">
                                    <PropertyValue Property="Label" String="{@i18n>SurveyQuestionApplLineItem}"/>
                                    <PropertyValue Property="ID" String="SurveyQuestionApplLineItem"/>
                                    <PropertyValue Property="Target" AnnotationPath="SurveyQuestionAppl/@UI.LineItem#SurveyQuestionApplLineItem"/>
                                </Record>
                            </Collection>
                        </PropertyValue>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="ComplianceReport"/>
                        <PropertyValue Property="Label" String="{@i18n>ComplianceReport}"/>
                        <PropertyValue Property="Target"  AnnotationPath="ComplianceReport/@UI.LineItem#FileList"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="{@i18n>ContactMechPurpose}"/>
                        <PropertyValue Property="ID" String="ContactMechPurpose"/>
                        <PropertyValue Property="Target" AnnotationPath="PostalAddress/@UI.LineItem#PostalAddressLineItem_NOAction"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="BeneficialPersonLineItem"/>
                        <PropertyValue Property="Label" String="{@i18n>BeneficialPersonLineItem}"/>
                        <PropertyValue Property="Target" AnnotationPath="BeneficialPerson/@UI.LineItem#BeneficialPersonLineItem"/>
                    </Record>
                </Collection>
                
            </Annotation>

            <Annotation Term="UI.Identification">
                <Collection > 
                    <Record Type="UI.DataFieldForAction" >
                        <PropertyValue Property="Action" String="com.dpbird.SubmitToProcurement"/>
                        <PropertyValue Property="Label"  String="{@i18n>SubmitApproval_Supplier}" />
                        <Annotation Term="UI.Hidden">
                            <If>
                                <Eq>
                                    <Path>currentStatusId</Path>
                                    <String>PROCESSED</String>
                                </Eq>
                                <Bool>true</Bool>
                                <Bool>false</Bool>
                            </If>
                        </Annotation>
                    </Record>    
                    <Record Type="UI.DataFieldForAction">
                        <PropertyValue Property="Action" String="com.dpbird.FillPartyClassification" />
                        <PropertyValue Property="Label" String="{@i18n>FillRisqueLevel}" />
                        <Annotation Term="UI.Hidden">
                            <If>
                                <Eq>
                                    <Path>currentStatusId</Path>
                                    <String>PROCESSED</String>
                                </Eq>
                                <Bool>true</Bool>
                                <Bool>false</Bool>
                            </If>
                        </Annotation>
                    </Record>                                                                                                                                                   
                </Collection>
            </Annotation>

            <Annotation Term="UI.SelectionPresentationVariant" Qualifier="tableView1">
                <Record Type="UI.SelectionPresentationVariantType">
                    <PropertyValue Property="PresentationVariant">
                        <Record Type="UI.PresentationVariantType">
                            <PropertyValue Property="Visualizations">
                                <Collection>
                                    <AnnotationPath>@UI.LineItem#ComplianceAppLineItem</AnnotationPath>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="SelectionVariant">
                        <Record Type="UI.SelectionVariantType">
                            <PropertyValue Property="SelectOptions">
                                <Collection>
                                    <Record Type="UI.SelectOptionType">
                                        <PropertyValue Property="PropertyName" PropertyPath="currentStatusId" />
                                        <PropertyValue Property="Ranges">
                                            <Collection>
                                                <Record Type="UI.SelectionRangeType">
                                                    <PropertyValue Property="Low" String="PROCESSED"></PropertyValue>
                                                    <PropertyValue Property="Option" EnumMember="UI.SelectionRangeOptionType/EQ" ></PropertyValue>
                                                    <PropertyValue Property="Sign" EnumMember="UI.SelectionRangeSignType/I" ></PropertyValue>
                                                </Record>
                                            </Collection>
                                        </PropertyValue>
                                    </Record>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="Text" String="{@i18n>Processed}"/>
                </Record>
            </Annotation>
            
            <Annotation Term="UI.SelectionPresentationVariant" Qualifier="tableView">
                <Record Type="UI.SelectionPresentationVariantType">
                    <PropertyValue Property="PresentationVariant">
                        <Record Type="UI.PresentationVariantType">
                            <PropertyValue Property="Visualizations">
                                <Collection>
                                    <AnnotationPath>@UI.LineItem#ComplianceAppLineItem</AnnotationPath>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="SelectionVariant">
                        <Record Type="UI.SelectionVariantType">
                            <PropertyValue Property="SelectOptions">
                                <Collection>
                                    <Record Type="UI.SelectOptionType">
                                        <PropertyValue Property="PropertyName" PropertyPath="currentStatusId" />
                                        <PropertyValue Property="Ranges">
                                            <Collection>
                                                <Record Type="UI.SelectionRangeType">
                                                    <PropertyValue Property="Low" String="NOT_PROCESSED"></PropertyValue>
                                                    <PropertyValue Property="Option" EnumMember="UI.SelectionRangeOptionType/EQ" ></PropertyValue>
                                                    <PropertyValue Property="Sign" EnumMember="UI.SelectionRangeSignType/I" ></PropertyValue>
                                                </Record>
                                            </Collection>
                                        </PropertyValue>
                                    </Record>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="Text" String="{@i18n>NotProcessed}"/>
                </Record>
            </Annotation>
            <Annotation Term="UI.FieldGroup" Qualifier="ViewDDForm">
                <Record Type="UI.FieldGroupType">
                    <PropertyValue Property="Data">
                        <Collection>
                            <Record Type="UI.DataFieldWithUrl">
                                <PropertyValue Property="Label" String=""/>
                                <PropertyValue Property="Value" Path="ddFromName"/>
                                <PropertyValue Property="Url" Path="ddFromUrl"/>
                            </Record>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
       </Annotations>

        <Annotations Target="com.dpbird.SubmitToProcurement/source">
            <Annotation Term="UI.ParameterDefaultValue" String="compliance"></Annotation>
            <Annotation Term="UI.Hidden" Bool="true"></Annotation>
        </Annotations>

         <Annotations Target="com.dpbird.SupplierParty">
            <Annotation Term="UI.DeleteHidden" Bool="true" />
        </Annotations>

        <Annotations Target="com.dpbird.FillPartyClassification">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>supplierParty</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.FillPartyClassification/additionalProtection">
            <Annotation Term="UI.MultiLineText" ></Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.FillPartyClassification/adverseResults">
            <Annotation Term="UI.MultiLineText" ></Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.CreateSupplierParty">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>supplierParty</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.SubmitToProcurement">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>supplierParty</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.PartyClassification">
            <Annotation Term="UI.FieldGroup" Qualifier="PartyClassificationFieldGroup">
                <Record Type="UI.FieldGroupType">
                    <PropertyValue Property="Data">
                        <Collection>
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Label" String="{@i18n>RiskRating}"/>
                                <PropertyValue Property="Value" Path="PartyClassificationGroup/description" />
                                <PropertyValue Property="Criticality" Path="statusColor" />
                            </Record>
                            
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>
        <Annotations Target="com.dpbird.PartyMediaResource">
            <Annotation Term="UI.LineItem" Qualifier="FileList">
                <Collection>
                    <Record Type="UI.DataFieldWithUrl">
                        <PropertyValue Property="Label" String="File"/>
                        <PropertyValue Property="Value" Path="contentName"/>
                        <PropertyValue Property="Url" Path="fileUrl"/>
                    </Record>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value">
                            <Path>createdDate</Path>
                        </PropertyValue>
                        <Annotation Term="UI.Importance">
                            <EnumMember>UI.ImportanceType/High</EnumMember>
                        </Annotation>
                    </Record>
                </Collection>
            </Annotation>
        </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
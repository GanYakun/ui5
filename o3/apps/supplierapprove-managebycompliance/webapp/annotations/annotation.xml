<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Common.v1" Alias="Common"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Namespace="com.sap.vocabularies.UI.v1" Alias="UI"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Communication.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Communication.v1" Alias="Communication"/>
    </edmx:Reference>
    <edmx:Reference Uri="/officeauto/control/odataAppSvc/supplierApproveService/$metadata">
        <edmx:Include Namespace="com.dpbird"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="local">
        <Annotations Target="com.dpbird.SupplierParty">
             <Annotation Term="UI.HeaderFacets">
                <Collection>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="PartyBaseInfo"/>
                        <PropertyValue Property="Label" String="{@i18n>PartyBaseInfo}"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#PartyBaseInfo"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="TimeInfo"/>
                        <PropertyValue Property="Label" String="{@i18n>TimeInfo}"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#TimeInfo"/>
                    </Record>
                        <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="statusId"/>
                        <PropertyValue Property="Target" AnnotationPath="@UI.DataPoint#statusId"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#IdentificationCode"/>
                        <PropertyValue Property="Label" String="{@i18n>Identification}"/>
                        <PropertyValue Property="ID" String="Identification"/>
                    </Record>
                </Collection>
            </Annotation>

            <Annotation Term="UI.Facets">
                <Collection>
                    <Record Type="UI.CollectionFacet">
                        <PropertyValue Property="Label" String="{@i18n>CompanyInfo}"/>
                        <PropertyValue Property="ID" String="CompanyInfo"/>
                        <PropertyValue Property="Facets">
                            <Collection>
                                <Record Type="UI.ReferenceFacet">
                                    <PropertyValue Property="Label" String="{@i18n>CompanyBaseInfo}"/>
                                    <PropertyValue Property="ID" String="PartyGroupFieldGroup"/>
                                    <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#PartyGroupFieldGroup"/>
                                </Record>
                                    <Record Type="UI.ReferenceFacet">
                                    <PropertyValue Property="Label" String="{@i18n>CompanyContactInfo}"/>
                                    <PropertyValue Property="ID" String="ContactInfo"/>
                                    <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#ContactInfo"/>
                                </Record>
                                <Record Type="UI.ReferenceFacet">
                                    <PropertyValue Property="Label" String="{@i18n>SupplierTimeInfo}"/>
                                    <PropertyValue Property="ID" String="SupplierTimeInfo"/>
                                    <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#SupplierTimeInfo"/>
                                </Record>
                            </Collection>
                        </PropertyValue>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="{@i18n>PartyClassificationGroupLineItem}"/>
                        <PropertyValue Property="ID" String="PartyClassificationGroupLineItem"/>
                        <PropertyValue Property="Target" AnnotationPath="PartyClassification/@UI.FieldGroup#PartyClassificationFieldGroup"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="{@i18n>SurveyQuestionApplLineItem}"/>
                        <PropertyValue Property="ID" String="SurveyQuestionApplLineItem"/>
                        <PropertyValue Property="Target" AnnotationPath="SurveyQuestionAppl/@UI.LineItem#SurveyQuestionApplLineItem"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="PartyRelationshipLineItem"/>
                        <PropertyValue Property="Label" String="{@i18n>PartyRelationshipLineItem}"/>
                        <PropertyValue Property="Target"  AnnotationPath="PersonAndContact/@UI.LineItem#PartyRelationshipLineItemWithoutAction"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="SupplierProductLineItem"/>
                        <PropertyValue Property="Label" String="{@i18n>SupplierProductLineItem}"/>
                        <PropertyValue Property="Target"  AnnotationPath="SupplierProduct/@UI.LineItem#SupplierProductLineItemWithoutAction"/>
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="PurchaseAgreementLineItem"/>
                        <PropertyValue Property="Label" String="{@i18n>PurchaseAgreementLineItem}"/>
                        <PropertyValue Property="Target"  AnnotationPath="PurchaseAgreement/@UI.LineItem#PurchaseAgreementLineItem"/>
                    </Record>
                        <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="Label" String="{@i18n>BankAccount}"/>
                        <PropertyValue Property="ID" String="BankAccount"/>
                        <PropertyValue Property="Target" AnnotationPath="OwnerFinAccount/@UI.LineItem#FinAccountLineItem" />
                    </Record>
                    <Record Type="UI.ReferenceFacet">
                        <PropertyValue Property="ID" String="NoteDataLineItem"/>
                        <PropertyValue Property="Label" String="{@i18n>NoteDataLineItem}"/>
                        <PropertyValue Property="Target"  AnnotationPath="NoteData/@UI.LineItem#NoteDataLineItem"/>
                    </Record>
                </Collection>
            </Annotation>

            <Annotation Term="UI.DataPoint" Qualifier="statusId">
                <Record Type="UI.DataPointType">
                    <PropertyValue Property="Value" Path="currentStatusId"/>
                    <PropertyValue Property="Title" String="{@i18n>statusId}"/>
                    <PropertyValue Property="Criticality" Path="criticalityValue"/>
                </Record>
            </Annotation>

            <Annotation Term="UI.Identification">
                <Collection > 
                    <Record Type="UI.DataFieldForAction" >
                        <PropertyValue Property="Action" String="com.dpbird.SubmitToProcurement"/>
                        <PropertyValue Property="Label"  String="{@i18n>SubmitApproval_Supplier}" />
                        <Annotation Term="UI.Hidden">
                            <If>
                                <Eq>
                                    <Path>currentStatusId</Path>
                                    <String>PROCESSED</String>
                                </Eq>
                                <Bool>true</Bool>
                                <Bool>false</Bool>
                            </If>
                        </Annotation>
                    </Record>                                                                                                                                                       
                </Collection>
            </Annotation>

            <Annotation Term="UI.SelectionPresentationVariant" Qualifier="tableView1">
                <Record Type="UI.SelectionPresentationVariantType">
                    <PropertyValue Property="PresentationVariant">
                        <Record Type="UI.PresentationVariantType">
                            <PropertyValue Property="Visualizations">
                                <Collection>
                                    <AnnotationPath>@UI.LineItem</AnnotationPath>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="SelectionVariant">
                        <Record Type="UI.SelectionVariantType">
                            <PropertyValue Property="SelectOptions">
                                <Collection>
                                    <Record Type="UI.SelectOptionType">
                                        <PropertyValue Property="PropertyName" PropertyPath="currentStatusId" />
                                        <PropertyValue Property="Ranges">
                                            <Collection>
                                                <Record Type="UI.SelectionRangeType">
                                                    <PropertyValue Property="Low" String="PROCESSED"></PropertyValue>
                                                    <PropertyValue Property="Option" EnumMember="UI.SelectionRangeOptionType/EQ" ></PropertyValue>
                                                    <PropertyValue Property="Sign" EnumMember="UI.SelectionRangeSignType/I" ></PropertyValue>
                                                </Record>
                                            </Collection>
                                        </PropertyValue>
                                    </Record>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="Text" String="{@i18n>Processed}"/>
                </Record>
            </Annotation>
            
            <Annotation Term="UI.SelectionPresentationVariant" Qualifier="tableView">
                <Record Type="UI.SelectionPresentationVariantType">
                    <PropertyValue Property="PresentationVariant">
                        <Record Type="UI.PresentationVariantType">
                            <PropertyValue Property="Visualizations">
                                <Collection>
                                    <AnnotationPath>@UI.LineItem</AnnotationPath>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="SelectionVariant">
                        <Record Type="UI.SelectionVariantType">
                            <PropertyValue Property="SelectOptions">
                                <Collection>
                                    <Record Type="UI.SelectOptionType">
                                        <PropertyValue Property="PropertyName" PropertyPath="currentStatusId" />
                                        <PropertyValue Property="Ranges">
                                            <Collection>
                                                <Record Type="UI.SelectionRangeType">
                                                    <PropertyValue Property="Low" String="NOT_PROCESSED"></PropertyValue>
                                                    <PropertyValue Property="Option" EnumMember="UI.SelectionRangeOptionType/EQ" ></PropertyValue>
                                                    <PropertyValue Property="Sign" EnumMember="UI.SelectionRangeSignType/I" ></PropertyValue>
                                                </Record>
                                            </Collection>
                                        </PropertyValue>
                                    </Record>
                                </Collection>
                            </PropertyValue>
                        </Record>
                    </PropertyValue>
                    <PropertyValue Property="Text" String="{@i18n>NotProcessed}"/>
                </Record>
            </Annotation>
       </Annotations>

        <Annotations Target="com.dpbird.SubmitToProcurement/target">
            <Annotation Term="UI.ParameterDefaultValue" String="procurement"></Annotation>
            <Annotation Term="UI.Hidden" Bool="true"></Annotation>
        </Annotations>

         <Annotations Target="com.dpbird.SupplierParty">
            <Annotation Term="UI.DeleteHidden" Bool="true" />
        </Annotations>

        <Annotations Target="com.dpbird.FillPartyClassification">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>partyClassification</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.CreateSupplierParty">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>supplierParty</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.SubmitToProcurement">
            <Annotation Term="Common.SideEffects">
                <Record>
                    <PropertyValue Property="TargetEntities">
                        <Collection>
                            <NavigationPropertyPath>supplierParty</NavigationPropertyPath>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>

        <Annotations Target="com.dpbird.PartyClassification">
            <Annotation Term="UI.FieldGroup" Qualifier="PartyClassificationFieldGroup">
                <Record Type="UI.FieldGroupType">
                    <PropertyValue Property="Data">
                        <Collection>
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Label" String="{@i18n>RiskRating}"/>
                                <PropertyValue Property="Value" Path="PartyClassificationGroup/description" />
                                <PropertyValue Property="Criticality" Path="statusColor" />
                            </Record>
                            <Record Type="UI.DataFieldForAction">
                                <PropertyValue Property="Action" String="com.dpbird.FillPartyClassification" />
                                <PropertyValue Property="Label" String="{@i18n>FillRisqueLevel}" />
                            </Record>
                        </Collection>
                    </PropertyValue>
                </Record>
            </Annotation>
        </Annotations>
        <Annotations Target="com.dpbird.FinAccount">
            <Annotation Term="UI.LineItem" Qualifier="FinAccountLineItem">
                <Collection>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value" Path="finAccountName"/>
                        <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
                    </Record>
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value" Path="finAccountCode"/>
                        <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
                    </Record>
                        <Record Type="UI.DataField">
                        <PropertyValue Property="Value" Path="currencyUomId"/>
                        <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
                    </Record>
                    <!-- <Record Type="UI.DataField">
                        <PropertyValue Property="Value" Path="accountName"/>
                        <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
                    </Record> -->
                    <Record Type="UI.DataField">
                        <PropertyValue Property="Value" Path="statusId"/>
                        <PropertyValue Property="Criticality">
                            <If>
                            <Or>
                            <Eq>
                                <Path>statusId</Path>
                                <String>FNACT_MANFROZEN</String>
                            </Eq>
                                <Eq>
                                <Path>statusId</Path>
                                <String>FNACT_CANCELLED</String>
                            </Eq>
                            </Or>
                                <Int>1</Int>
                            <Int>3</Int>
                        </If>
                        </PropertyValue>
                        <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High"/>
                    </Record>
                </Collection>
            </Annotation>
        </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>